{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "austinb/gameq": "^3.0",
        "barryvdh/laravel-ide-helper": "^2",
        "corbpie/bunny-cdn-api": "^1.9",
        "deployer/recipes": "^6.0",
        "doctrine/dbal": "^3",
        "fakerphp/faker": "^1",
        "graham-campbell/github": "^12",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "inertiajs/inertia-laravel": "^v1",
        "kanalumaddela/laravel-steam-login": "dev-master",
        "laravel/framework": "^v10",
        "laravel/pint": "^v1.18.3",
        "laravel/sail": "^1.14",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^7",
        "platformcommunity/flysystem-bunnycdn": "^3",
        "sentry/sentry-laravel": "^4.7.0",
        "servo/fluidxml": "^1.20.3",
        "shetabit/token-builder": "^v2.0.0",
        "spatie/laravel-csp": "^2.7",
        "spatie/laravel-ignition": "^2.0",
        "tightenco/ziggy": "^1.4",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.0",
        "laravel/dusk": "^8",
        "pestphp/pest": "^2",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
