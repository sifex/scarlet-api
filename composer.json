{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "austinb/gameq": "~3.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "deployer/recipes": "^6.0",
        "doctrine/dbal": "^2.10",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "graham-campbell/github": "^10.5",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "inertiajs/inertia-laravel": "0.6.9",
        "kanalumaddela/laravel-steam-login": "^2.5",
        "knplabs/github-api": "^3.0",
        "laravel/framework": "^v9.29.0",
        "laravel/octane": "^1.3",
        "laravel/pint": "^0.1.7",
        "laravel/sail": "^1.14",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.0",
        "sentry/sentry-laravel": "^3.3",
        "servo/fluidxml": "^1.20.3",
        "shetabit/token-builder": "^1.2",
        "spatie/laravel-csp": "^2.7",
        "spatie/laravel-ignition": "^1.0",
        "spiral/roadrunner": "^2.12",
        "tightenco/ziggy": "^1.4",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.0",
        "laravel/dusk": "^6.24"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
